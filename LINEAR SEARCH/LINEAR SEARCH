#include <iostream>
using namespace std;
bool linear_search(int arr[],int size,int key){

for(int i=0;i<=size;i++){

    if(arr[i]==key){
        return 1;
    }
       
    
     }
     return 0;
}
int main() {
int arr[8]={2,3,4,5,1,2,3,4};
int key;

cout<<"enter the you want to search for";
cin>>key;

bool found= linear_search(arr,8,key);
if(found){
    cout<<"KEY IS PRESENT";
    
}
    else{
        cout<<"key is not present";
    }
    return 0;
    }




