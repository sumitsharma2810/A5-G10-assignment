#include<iostream>
using namespace std;
int main()
{
    int n, i, arr[50], j, sumit;
    cout<<"Enter the Size (max. 50): ";
    cin>>n;
    cout<<"Enter "<<n<<" Numbers: ";
    for(i=0; i<n; i++)
        cin>>arr[i];
   
    for(i=0; i<(n-1); i++)
    {
        for(j=0; j<(n-i-1); j++)
        {
            if(arr[j]>arr[j+1])
            {
                sumit = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = sumit;
            }
        }
    }
    for(i=0; i<n; i++)
        cout<<arr[i]<<" ";
    cout<<endl;
    return 0;
}
